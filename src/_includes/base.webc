			font-display: swap;
			src: url(/fonts/UcCO3FwrK3iLTeHuS_nVMrMxCp50SjIw2boKoduKmMEVuLyfMZg.ttf) format('truetype');
		}
		@font-face {
			font-family: 'Inter';
			font-style: normal;
			font-weight: 500;
