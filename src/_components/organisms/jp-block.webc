<script webc:type="js">
  const kebabCase = (str) => str.replace(/([a-z])([A-Z])/g, '$1-$2').toLowerCase();
  const namespace = (str) => str.match(/[A-Z]/g).join('').toLowerCase();
  const library = webc.attributes.block.library || 'FlowBite';
  `<x webc:is="${namespace(library)}-${kebabCase(webc.attributes.block.type)}" :object="webc.attributes.block"></x>`
</script>
